{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "1.7.3",
        "drupal/accordion_blocks": "^2.0",
        "drupal/admin_audit_trail": "^1.0",
        "drupal/admin_toolbar": "^3.6",
        "drupal/auto_entitylabel": "3.0",
        "drupal/az_blob_fs": "^2.3",
        "drupal/barcodes": "2.0.6",
        "drupal/better_exposed_filters": "6.0.3",
        "drupal/block_class": "^3.0",
        "drupal/bookmarks": "1.0.2",
        "drupal/bootstrap5": "^4.0",
        "drupal/captcha": "2.0.5",
        "drupal/chosen": "^4.0",
        "drupal/clientside_validation": "^4.1",
        "drupal/coder": "^8.3",
        "drupal/collapsiblock": "^4.1",
        "drupal/config_entity_cloner": "^1.1@RC",
        "drupal/config_filter": "^2.6",
        "drupal/config_ignore": "^3.1",
        "drupal/config_split": "^2.0",
        "drupal/cookies": "1.2.8",
        "drupal/core": "10.5.1",
        "drupal/core-composer-scaffold": "10.5.1",
        "drupal/core-project-message": "10.5.1",
        "drupal/core-recommended": "10.5.1",
        "drupal/cshs": "^4.0",
        "drupal/csv_importer": "1.16",
        "drupal/ctools": "4.0.4",
        "drupal/date_popup": "2.0.1",
        "drupal/delete_all": "^3.0",
        "drupal/devel": "^5.4",
        "drupal/devel_kint_extras": "1.1.0",
        "drupal/diff": "^1.9",
        "drupal/ds": "^3.19",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/elasticsearch_connector": "7.0-alpha4",
        "drupal/entity_print": "2.13",
        "drupal/events_log_track": "^3.1",
        "drupal/extlink": "^2.0",
        "drupal/facets": "3.0.0-beta1",
        "drupal/feeds": "^3.0@beta",
        "drupal/feeds_para_mapper": "^1.1",
        "drupal/feeds_tamper": "2.0-beta3",
        "drupal/field_group": "3.4",
        "drupal/field_permissions": "^1.4",
        "drupal/file_extractor": "^4.1",
        "drupal/google_analytics": "4.0.2",
        "drupal/govuk_cookies": "1.0.x-dev@dev",
        "drupal/govuk_theme": "^1.16",
        "drupal/hierarchical_term_formatter": "^1.3",
        "drupal/honeypot": "2.1.3",
        "drupal/ldap_auth": "^2.0",
        "drupal/link_attributes": "2.1.0",
        "drupal/login_redirect_per_role": "^1.9",
        "drupal/mailsystem": "4.4.0",
        "drupal/menu_block": "1.10",
        "drupal/message_notify": "^1.3",
        "drupal/metatag": "^2.0",
        "drupal/miniorange_saml": "^3.0",
        "drupal/miniorange_saml_idp": "^3.0",
        "drupal/openid_connect": "^3.0@alpha",
        "drupal/paragraphs": "^1.17",
        "drupal/pathauto": "1.12",
        "drupal/quick_node_clone": "^1.17",
        "drupal/rabbit_hole": "2.0.0-alpha5",
        "drupal/recaptcha": "3.2.0",
        "drupal/recaptcha_v3": "2.0.02",
        "drupal/recently_read": "^1.3",
        "drupal/redirect": "^1.9",
        "drupal/redirect_after_login": "^2.7",
        "drupal/saml_sp": "^4.2",
        "drupal/samlauth": "^3.9",
        "drupal/scan_code": "1.0-alpha8",
        "drupal/schema_metatag": "^3.0",
        "drupal/search_api": "1.31",
        "drupal/search_api_attachments": "9.0.2",
        "drupal/search_api_saved_searches": "^1.0@beta",
        "drupal/search_api_solr": "^4.3",
        "drupal/shs": "^2.0@RC",
        "drupal/stringoverrides": "1.8",
        "drupal/structure_sync": "^2.0",
        "drupal/symfony_mailer": "1.4.0",
        "drupal/tamper": "1.0-alpha4",
        "drupal/taxonomy_delete": "^1.5",
        "drupal/taxonomy_manager": "2.0.9",
        "drupal/taxonomy_menu": "^3.6",
        "drupal/taxonomy_multidelete_terms": "^1.4",
        "drupal/token": "1.13",
        "drupal/trim": "^2.0",
        "drupal/twig_tweak": "^3.4",
        "drupal/user_default_page": "9.0.1",
        "drupal/vbo_export": "^4.1",
        "drupal/views_accordion": "2.0.1",
        "drupal/views_bulk_edit": "^3.0",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/views_contextual_filters_or": "^1.4",
        "drupal/views_data_export": "^1.4",
        "drupal/views_send": "^2.0@RC",
        "drupal/webform": "6.2.0",
        "drupal/weight": "3.4",
        "drush/drush": "^12.4",
        "jjj/chosen": "^2.2",
        "vlucas/phpdotenv": "^5.1",
        "webflo/drupal-finder": "^1.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
	"audit": {
	    "block-insecure": false
	},
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "cweagans/composer-patches": true
        },
        "sort-packages": true,
        "discard-changes": true
        
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]  
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/elasticsearch_connector_views": {
                "Ignore Elasticsearch system indexes from Views": "https://www.drupal.org/files/issues/2021-08-25/elasticsearch_connector-hide-system-indices-3229401.patch"
            },
            "drupal/core" : {
                "access check error fix" : "https://www.drupal.org/files/issues/2023-12-08/core-patch.patch"
            }         
        },
        "enable-patching": true
    }
}
